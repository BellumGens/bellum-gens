{
  "name": "bellum-gens",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run start-bellumgens\" \"npm run start-ebleague\"",
    "start-bellumgens": "ng serve -o",
    "start-ebleague": "ng serve eb-league --port=4201 -o",
    "build": "ng build bellum-gens --configuration production && ng build eb-league --configuration production",
    "build:app-shell": "ng run bellum-gens:app-shell:production",
    "test": "ng test",
    "test:prod": "ng test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.bellumgens.server.config.js --progress --colors && webpack --config webpack.ebleague.server.config.js --progress --colors",
    "dev:ssr": "concurrently --kill-others \"npm run dev:ssr:bellumgens\" \"npm run dev:ssr:ebleague\"",
    "dev:ssr:bellumgens": "ng run bellum-gens:serve-ssr",
    "dev:ssr:ebleague": "ng run eb-league:serve-ssr",
    "serve:ssr": "concurrently --kill-others \"npm run serve:ssr:bellumgens\" \"npm run serve:ssr:ebleague\"",
    "serve:ssr:bellumgens": "cd dist/bellumgens && node server/main.js",
    "serve:ssr:ebleague": "cd dist/ebleague && node server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "npm run build && ng run bellum-gens:server:production && ng run eb-league:server:production",
    "build:prerender": "npm run build:ssr && node dist/prerender",
    "i18n": "npm run i18n:bellumgens && npm run i18n:ebleague",
    "i18n:bellumgens": "ng extract-i18n bellum-gens --out-file src-bellumgens/locale/messages.en.xlf",
    "i18n:ebleague": "ng extract-i18n eb-league --out-file src-ebleague/locale/messages.en.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.0.1",
    "@angular/common": "13.0.1",
    "@angular/compiler": "13.0.1",
    "@angular/core": "13.0.1",
    "@angular/forms": "13.0.1",
    "@angular/platform-browser": "13.0.1",
    "@angular/platform-browser-dynamic": "13.0.1",
    "@angular/platform-server": "13.0.1",
    "@angular/router": "13.0.1",
    "@angular/service-worker": "13.0.1",
    "@igniteui/material-icons-extended": "^2.10.0",
    "@infragistics/igniteui-angular": "13.0.0-rc.0",
    "@nguniversal/builders": "13.0.1",
    "@nguniversal/express-engine": "13.0.1",
    "@types/express": "^4.17.11",
    "class": "^0.1.4",
    "compression": "^1.7.4",
    "core-js": "^2.6.12",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "jszip": "^3.6.0",
    "minireset.css": "0.0.4",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.2",
    "@angular-eslint/builder": "^13.0.0-alpha.0",
    "@angular-eslint/eslint-plugin": "^12.6.1",
    "@angular-eslint/eslint-plugin-template": "^12.6.1",
    "@angular-eslint/schematics": "^13.0.0-alpha.0",
    "@angular-eslint/template-parser": "^12.6.1",
    "@angular/cli": "~13.0.2",
    "@angular/compiler-cli": "^13.0.1",
    "@angular/language-service": "^13.0.1",
    "@angular/localize": "^13.0.1",
    "@types/hammerjs": "^2.0.39",
    "@types/jasmine": "^3.10.2",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.20.36",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "igniteui-cli": "~8.3.0-rc.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "4.4.4",
    "webpack-cli": "^3.1.0"
  }
}
