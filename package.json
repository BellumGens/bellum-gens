{
  "name": "bellum-gens",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "concurrently --kill-others \"npm run start-bellumgens\" \"npm run start-ebleague\"",
    "start-bellumgens": "ng serve -o",
    "start-ebleague": "ng serve eb-league --port=4201 -o",
    "build": "ng build --prod && ng build eb-league --prod",
    "build:app-shell": "ng run bellum-gens:app-shell:production",
    "test": "ng test",
    "test:prod": "ng test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.bellumgens.server.config.js --progress --colors && webpack --config webpack.ebleague.server.config.js --progress --colors",
    "dev:ssr": "concurrently --kill-others \"npm run dev:ssr:bellumgens\" \"npm run dev:ssr:ebleague\"",
    "dev:ssr:bellumgens": "ng run bellum-gens:serve-ssr",
    "dev:ssr:ebleague": "ng run eb-league:serve-ssr",
    "serve:ssr": "concurrently --kill-others \"npm run serve:ssr:bellumgens\" \"npm run serve:ssr:ebleague\"",
    "serve:ssr:bellumgens": "cd dist/bellumgens && node server/main.js",
    "serve:ssr:ebleague": "cd dist/ebleague && node server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "npm run build && ng run bellum-gens:server:production && ng run eb-league:server:production",
    "build:prerender": "npm run build:ssr && node dist/prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.14",
    "@angular/common": "10.0.14",
    "@angular/compiler": "10.0.14",
    "@angular/core": "10.0.14",
    "@angular/forms": "10.0.14",
    "@angular/platform-browser": "10.0.14",
    "@angular/platform-browser-dynamic": "10.0.14",
    "@angular/platform-server": "10.0.14",
    "@angular/router": "10.0.14",
    "@angular/service-worker": "10.0.14",
    "@igniteui/material-icons-extended": "^1.1.1",
    "@infragistics/igniteui-angular": "10.1.2",
    "@nguniversal/builders": "10.0.2",
    "@nguniversal/express-engine": "10.0.2",
    "@types/express": "^4.17.2",
    "class": "^0.1.4",
    "core-js": "^2.6.3",
    "domino": "^2.1.3",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "jszip": "^3.1.5",
    "minireset.css": "0.0.4",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "^10.0.14",
    "@angular/language-service": "^10.0.14",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.0.0",
    "igniteui-cli": "~6.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "3.9.5",
    "webpack-cli": "^3.1.0"
  }
}
