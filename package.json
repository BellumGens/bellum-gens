{
  "name": "bellum-gens",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run start-bellumgens\" \"npm run start-ebleague\"",
    "start-bellumgens": "ng serve bellum-gens --port=4200 -o",
    "start-ebleague": "ng serve eb-league --port=4201 -o",
    "build": "ng build common --configuration production && ng build bellum-gens --configuration production && ng build eb-league --configuration production",
    "buildapps": "ng build bellum-gens --configuration production && ng build eb-league --configuration production",
    "test": "ng test",
    "test:prod": "ng test --watch=false --no-progress --code-coverage --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.bellumgens.server.config.js --progress --colors && webpack --config webpack.ebleague.server.config.js --progress --colors",
    "dev:ssr": "concurrently --kill-others \"npm run dev:ssr:bellumgens\" \"npm run dev:ssr:ebleague\"",
    "dev:ssr:bellumgens": "ng run bellum-gens:serve-ssr",
    "dev:ssr:ebleague": "ng run eb-league:serve-ssr",
    "serve:ssr": "concurrently --kill-others \"npm run serve:ssr:bellumgens\" \"npm run serve:ssr:ebleague\"",
    "serve:ssr:bellumgens": "cd dist/bellumgens && node server/main.js",
    "serve:ssr:ebleague": "cd dist/ebleague && node server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "npm run buildapps && ng run bellum-gens:server:production && ng run eb-league:server:production",
    "build:prerender": "ng run bellum-gens:prerender && ng run eb-league:prerender",
    "i18n": "npm run i18n:bellumgens && npm run i18n:ebleague",
    "i18n:bellumgens": "ng extract-i18n bellum-gens --out-file projects/bellumgens/src/locale/messages.en.xlf",
    "i18n:ebleague": "ng extract-i18n eb-league --out-file projects/ebleague/src/locale/messages.en.xlf",
    "cypress:open": "concurrently \"ng serve\" \"cypress open --config-file=cypress.config.ts\"",
    "cypress:run": "concurrently \"ng serve\" \"cypress run --config-file=cypress.config.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.4",
    "@angular/common": "16.2.4",
    "@angular/compiler": "16.2.4",
    "@angular/core": "16.2.4",
    "@angular/forms": "16.2.4",
    "@angular/platform-browser": "16.2.4",
    "@angular/platform-browser-dynamic": "16.2.4",
    "@angular/platform-server": "16.2.4",
    "@angular/router": "16.2.4",
    "@angular/service-worker": "16.2.4",
    "@igniteui/material-icons-extended": "^3.0.2",
    "@infragistics/igniteui-angular": "16.0.22",
    "@nguniversal/builders": "16.2.0",
    "@nguniversal/express-engine": "16.2.0",
    "@types/express": "^4.17.17",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "hammerjs": "^2.0.8",
    "igniteui-angular-i18n": "^16.0.21",
    "minireset.css": "0.0.4",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.2",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-eslint/builder": "^16.1.2",
    "@angular-eslint/eslint-plugin": "^16.1.2",
    "@angular-eslint/eslint-plugin-template": "^16.1.2",
    "@angular-eslint/schematics": "^16.1.2",
    "@angular-eslint/template-parser": "^16.1.2",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "^16.2.4",
    "@angular/language-service": "^16.2.4",
    "@angular/localize": "^16.2.4",
    "@cypress/schematic": "^2.5.0",
    "@types/hammerjs": "^2.0.39",
    "@types/jasmine": "^4.3.5",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^18.17.14",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "concurrently": "^8.2.1",
    "cypress": "^13.1.0",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "46.2.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "igniteui-cli": "~12.0.5",
    "jasmine-core": "~5.1.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "ng-packagr": "^16.2.2",
    "typescript": "5.1.6"
  }
}