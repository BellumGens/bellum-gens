{
  "name": "bellum-gens",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run start-bellumgens\" \"npm run start-ebleague\"",
    "start-bellumgens": "ng serve bellum-gens --port=4200 -o",
    "start-ebleague": "ng serve eb-league --port=4201 -o",
    "build": "ng build common --configuration production && ng build bellum-gens --configuration production && ng build eb-league --configuration production",
    "build:app-shell": "ng run bellum-gens:app-shell:production",
    "test": "ng test",
    "test:prod": "ng test --watch=false --no-progress --code-coverage --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.bellumgens.server.config.js --progress --colors && webpack --config webpack.ebleague.server.config.js --progress --colors",
    "dev:ssr": "concurrently --kill-others \"npm run dev:ssr:bellumgens\" \"npm run dev:ssr:ebleague\"",
    "dev:ssr:bellumgens": "ng run bellum-gens:serve-ssr",
    "dev:ssr:ebleague": "ng run eb-league:serve-ssr",
    "serve:ssr": "concurrently --kill-others \"npm run serve:ssr:bellumgens\" \"npm run serve:ssr:ebleague\"",
    "serve:ssr:bellumgens": "cd dist/bellumgens && node server/main.js",
    "serve:ssr:ebleague": "cd dist/ebleague && node server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "npm run build && ng run bellum-gens:server:production && ng run eb-league:server:production",
    "build:prerender": "npm run build:ssr && node dist/prerender",
    "i18n": "npm run i18n:bellumgens && npm run i18n:ebleague",
    "i18n:bellumgens": "ng extract-i18n bellum-gens --out-file projects/bellumgens/src/locale/messages.en.xlf",
    "i18n:ebleague": "ng extract-i18n eb-league --out-file projects/ebleague/src/locale/messages.en.xlf",
    "cypress:open": "concurrently \"ng serve\" \"cypress open --config-file=cypress.config.ts\"",
    "cypress:run": "concurrently \"ng serve\" \"cypress run --config-file=cypress.config.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.5",
    "@angular/common": "15.1.5",
    "@angular/compiler": "15.1.5",
    "@angular/core": "15.1.5",
    "@angular/forms": "15.1.5",
    "@angular/platform-browser": "15.1.5",
    "@angular/platform-browser-dynamic": "15.1.5",
    "@angular/platform-server": "15.1.5",
    "@angular/router": "15.1.5",
    "@angular/service-worker": "15.1.5",
    "@igniteui/material-icons-extended": "^2.11.0",
    "@infragistics/igniteui-angular": "15.0.12",
    "@nguniversal/builders": "15.1.0",
    "@nguniversal/express-engine": "15.1.0",
    "@types/express": "^4.17.15",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "hammerjs": "^2.0.8",
    "minireset.css": "0.0.4",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@angular/language-service": "^15.1.5",
    "@angular/localize": "^15.1.5",
    "@cypress/schematic": "^2.3.0",
    "@types/hammerjs": "^2.0.39",
    "@types/jasmine": "^4.3.0",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "concurrently": "^7.6.0",
    "cypress": "^12.3.0",
    "eslint": "^8.32.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "37.9.2",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "igniteui-cli": "~11.0.2",
    "jasmine-core": "~4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-spec-reporter": "^0.0.36",
    "ng-packagr": "^15.1.1",
    "typescript": "4.9.5"
  }
}