{
  "name": "bellum-gens",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run start-bellumgens\" \"npm run start-ebleague\"",
    "start-bellumgens": "ng serve bellum-gens --port=4200 -o",
    "start-ebleague": "ng serve eb-league --port=4201 -o",
    "build": "npm run build:common && npm run build:apps",
    "build:common": "ng build common --configuration production",
    "build:bellumgens": "ng build bellum-gens --configuration production",
    "build:ebleague": "ng build eb-league --configuration production",
    "build:apps": "npm run build:bellumgens && npm run build:ebleague",
    "test": "ng test",
    "test:prod": "ng test --watch=false --no-progress --code-coverage --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr:bellumgens": "ng run bellum-gens:serve-ssr",
    "dev:ssr:ebleague": "ng run eb-league:serve-ssr",
    "serve:ssr": "concurrently --kill-others \"npm run serve:ssr:bellumgens\" \"npm run serve:ssr:ebleague\"",
    "serve:ssr:bellumgens": "cd dist/bellumgens && node server/main.js",
    "serve:ssr:ebleague": "cd dist/ebleague && node server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "npm run build:apps && ng run bellum-gens:server:production && ng run eb-league:server:production",
    "build:prerender": "ng run bellum-gens:prerender && ng run eb-league:prerender",
    "i18n": "npm run i18n:bellumgens && npm run i18n:ebleague",
    "i18n:bellumgens": "ng extract-i18n bellum-gens --out-file projects/bellumgens/src/locale/messages.en.xlf",
    "i18n:ebleague": "ng extract-i18n eb-league --out-file projects/ebleague/src/locale/messages.en.xlf",
    "cypress:open": "concurrently \"ng serve\" \"cypress open --config-file=cypress.config.ts\"",
    "cypress:run": "concurrently \"npm run serve:ssr:bellumgens\" \"cypress run --config-file=cypress.config.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.0.5",
    "@angular/common": "18.0.5",
    "@angular/compiler": "18.0.5",
    "@angular/core": "18.0.5",
    "@angular/forms": "18.0.5",
    "@angular/platform-browser": "18.0.5",
    "@angular/platform-browser-dynamic": "18.0.5",
    "@angular/platform-server": "18.0.5",
    "@angular/router": "18.0.5",
    "@angular/service-worker": "18.0.5",
    "@angular/ssr": "^18.0.6",
    "@igniteui/material-icons-extended": "^3.0.2",
    "@infragistics/igniteui-angular": "18.0.3",
    "@types/express": "^4.17.17",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "hammerjs": "^2.0.8",
    "igniteui-angular-i18n": "18.0.3",
    "minireset.css": "0.0.4",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "^18.0.6",
    "@angular/compiler-cli": "^18.0.5",
    "@angular/language-service": "^18.0.5",
    "@angular/localize": "^18.0.5",
    "@cypress/schematic": "^2.5.0",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "browser-sync": "^3.0.0",
    "concurrently": "^8.2.1",
    "cypress": "^13.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "46.2.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "igniteui-cli": "~13.3.0",
    "jasmine-core": "~5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "ng-packagr": "^18.0.0",
    "typescript": "5.4.5"
  }
}
