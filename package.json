{
  "name": "bellum-gens",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "concurrently --kill-others \"npm run start-bellumgens\" \"npm run start-ebleague\"",
    "start-bellumgens": "ng serve -o",
    "start-ebleague": "ng serve eb-league --port=4201 -o",
    "build": "ng build bellum-gens --prod && ng build eb-league --prod",
    "build:app-shell": "ng run bellum-gens:app-shell:production",
    "test": "ng test",
    "test:prod": "ng test -- --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.bellumgens.server.config.js --progress --colors && webpack --config webpack.ebleague.server.config.js --progress --colors",
    "dev:ssr": "concurrently --kill-others \"npm run dev:ssr:bellumgens\" \"npm run dev:ssr:ebleague\"",
    "dev:ssr:bellumgens": "ng run bellum-gens:serve-ssr",
    "dev:ssr:ebleague": "ng run eb-league:serve-ssr",
    "serve:ssr": "concurrently --kill-others \"npm run serve:ssr:bellumgens\" \"npm run serve:ssr:ebleague\"",
    "serve:ssr:bellumgens": "cd dist/bellumgens && node server/main.js",
    "serve:ssr:ebleague": "cd dist/ebleague && node server/main.js",
    "build:ssr": "npm run build:client-and-server-bundles",
    "build:client-and-server-bundles": "npm run build && ng run bellum-gens:server:production && ng run eb-league:server:production",
    "build:prerender": "npm run build:ssr && node dist/prerender",
    "i18n": "npm run i18n:bellumgens && npm run i18n:ebleague",
    "i18n:bellumgens": "ng extract-i18n bellum-gens --out-file src-bellumgens/locale/messages.en.xlf",
    "i18n:ebleague": "ng extract-i18n eb-league --out-file src-ebleague/locale/messages.en.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/platform-server": "12.0.0",
    "@angular/router": "12.0.0",
    "@angular/service-worker": "12.0.0",
    "@igniteui/material-icons-extended": "^2.7.0",
    "@infragistics/igniteui-angular": "11.2.0-alpha.11",
    "@nguniversal/builders": "12.0.0",
    "@nguniversal/express-engine": "12.0.0",
    "@types/express": "^4.17.11",
    "class": "^0.1.4",
    "core-js": "^2.6.12",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "jszip": "^3.6.0",
    "minireset.css": "0.0.4",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "web-animations-js": "^2.3.2",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/builder": "^4.0.0",
    "@angular-eslint/eslint-plugin": "^4.0.0",
    "@angular-eslint/eslint-plugin-template": "^4.0.0",
    "@angular-eslint/schematics": "^4.3.0",
    "@angular-eslint/template-parser": "^4.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@angular/language-service": "^12.0.0",
    "@angular/localize": "^12.0.0",
    "@types/hammerjs": "^2.0.39",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.20.10",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "igniteui-cli": "~7.1.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "4.2.4",
    "webpack-cli": "^3.1.0"
  }
}
