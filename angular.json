            "polyfills": "src-ebleague/polyfills.ts",
            "tsConfig": "src-ebleague/tsconfig.spec.json",
            "karmaConfig": "src-ebleague/karma.conf.js",
            "styles": [
              "src-ebleague/styles.scss"
            ],
            "scripts": [
              "./node_modules/hammerjs/hammer.min.js"
            ],
            "assets": [
